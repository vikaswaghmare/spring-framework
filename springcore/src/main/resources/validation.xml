<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:context="http://www.springframework.org/schema/context"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd
 http://www.springframework.org/schema/context
 http://www.springframework.org/schema/context/spring-context.xsd">
 
 <bean id="emp" class="com.springcore.SpringValidations.Employee">
<property name="eid" value="1"/>
<property name="ename" value="d"/>
 <property name="esal" value="12"/>
 <property name="eage" value="17"/>
 <property name="eemail" value="viks@gmail.com"/>
 <property name="emobile" value="12345678964"/>
 </bean>
 <bean id="empValidator" class="com.springcore.SpringValidations.EmployeeValidator">
	 <property name="resource" value="messages.properties"/>
 </bean>
 
 
 </beans>